{
    "name": "bugbulltech/spedmdfe",
    "type": "library",
    "description": "API para geração e comunicação da MDFe com as SEFAZ autorizadoras.",
    "keywords": ["mdfe", "sped", "nfephp"],
    "homepage": "https://github.com/nfephp-org/sped-mdfe",
    "license": ["GPL-3.0+","LGPL-3.0+","MIT"],
    "require": {
        "php" : "~5.5|~7.0",
        "nfephp-org/sped-common" : "^4.1",
        "zendframework/zend-mail": "^2.5",
        "html2text/html2text": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
          "NFePHP\\MDFe\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NFePHP\\MDFe\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.0-dev"
        }
    },
    "minimum-stability": "dev"
}
